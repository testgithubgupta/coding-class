#include <stdio.h>

int main()
{
    int i,j,k,n=5;
    for(i=0;i<=5;i++)
    {
        for(j=1;j<=(n-i);j++)
       {
           printf(" ");
       }
       for(k=1;k<=i;k++)
       {
           printf("*");
           
       }
       printf("\n");
    }

    return 0;
}
